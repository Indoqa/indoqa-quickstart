{
  "name": "quickstart-boot-frontend",
  "version": "0.0.0",
  "description": "Quickstart Indoqa Boot: Frontend",
  "license": "UNLICENSED",
  "private": true,
  "scripts": {
    "start": "node ./src/dev/server.js",
    "lint": "indoqa-eslint ./src/main",
    "package": "yarn run lint & indoqa-webpack indoqa-webpack-options.js",
    "test": "cross-env NODE_ENV=test mocha ./src/test --recursive --compilers js:babel-register --require ./src/test/setup.js",
    "test:watch": "yarn test -- --watch",
    "postinstall": "yarn run package"
  },
  "dependencies": {
    "immutable": "^3.7.6",
    "indoqa-react-app": "^0.1.6",
    "indoqa-react-restclient": "^0.1.0",
    "react": "^15.4.1",
    "react-dom": "^15.4.1",
    "react-redux": "^5.0.1",
    "react-router": "^3.0.0",
    "redux": "^3.1.2",
    "redux-logger": "^2.6.1",
    "redux-multi": "^0.1.12",
    "redux-promise-middleware": "^4.0.0"
  },
  "devDependencies": {
    "cross-env": "^3.1.3",
    "expect": "^1.20.2",
    "indoqa-webpack": "0.7.9",
    "jsdom": "^9.4.1",
    "mocha": "^3.0.1"
  }
}
